                <term_definitions code="at0001">
                    <items id="text">history</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0002">
                    <items id="text">Any event</items>
                    <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
                </term_definitions>
                <term_definitions code="at0003">
                    <items id="text">Simple</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0004">
                    <items id="text">Height/Length</items>
                    <items id="description">The length of the body from crown of head to sole of foot.</items>
                </term_definitions>
                <term_definitions code="at0007">
                    <items id="text">List</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0011">
                    <items id="text">Device</items>
                    <items id="description">Description of the device used to measure height or body length.</items>
                </term_definitions>
                <term_definitions code="at0013">
                    <items id="text">Tree</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0014">
                    <items id="text">Position</items>
                    <items id="description">Position of individual when measured.</items>
                </term_definitions>
                <term_definitions code="at0016">
                    <items id="text">Standing</items>
                    <items id="description">Height is optimally measured without shoes, without socks (to be able to properly assess the position of the foot), buttocks, heels and shoulders in contact with the wall, the head in the Frankfurt position (lower orbital rim horizontal with the external acoustic meatus). The individual should be told to "stand straight", but not to push their head up.</items>
                </term_definitions>
                <term_definitions code="at0018">
                    <items id="text">Comment</items>
                    <items id="description">Comment about the measurement of body height/length.</items>
                </term_definitions>
                <term_definitions code="at0019">
                    <items id="text">Confounding factors</items>
                    <items id="description">Record any issues or factors that may impact on the measurement of body height/length eg noting of amputation, hat, hairdo, etc.</items>
                </term_definitions>
                <term_definitions code="at0020">
                    <items id="text">Lying</items>
                    <items id="description">Length is measured in a fully extended, recumbent position with the pelvis flat, legs extended and feet flexed.</items>
                </term_definitions>
                <term_definitions code="at0021">
                    <items id="text">Birth</items>
                    <items id="description">The first length measured after birth. This event should only be used once per record.</items>
                </term_definitions>
                <term_definitions code="at0025">
                    <items id="text">Adjustment method</items>
                    <items id="description">Formula used to calculate the Adjusted height/length.</items>
                </term_definitions>
                <term_definitions code="at0026">
                    <items id="text">Adjusted height/length</items>
                    <items id="description">Adjusted height/length is calculated based on a measured height/length, armspan, underarm length, etc. Adjused height/length is used among other things to calculate BMI of patients with missing, injured or malformed body parts, which significantly affects measured height/length.</items>
                </term_definitions>
                <term_definitions code="at0028">
                    <items id="text">Justification for adjustment</items>
                    <items id="description">Justification for why the element "Adjusted height/length" is used.</items>
                </term_definitions>
                <term_definitions code="at0000">
                    <items id="text">Height/Length</items>
                    <items id="description">The height or length of the individual, measured from crown of head to sole of foot. Height is measured with the individual in a standing position and body length in a recumbent position.</items>
                </term_definitions>
                <term_definitions code="at">
                    <items id="text">Lying</items>
                    <items id="description">Length is measured in a fully extended, recumbent position with the pelvis flat, legs extended and feet flexed.</items>
                </term_definitions>
                <term_definitions code="cm">
                    <items id="text">centimeter</items>
                    <items id="description">@ unit @</items>
                </term_definitions>
            </children>
